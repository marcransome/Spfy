<?xml version="1.0" encoding="UTF-8"?>
<playlist version='1' xmlns='http://xspf.org/ns/0/'>
  <title><%= title.to_s.encode(:xml => :text) %></title>
  <creator><%= creator.to_s.encode(:xml => :text) %></creator>
  <date><%= date.to_s.encode(:xml => :text) %></date>
  <annotation><%= annotation.to_s.encode(:xml => :text) %></annotation>
  <trackList>
      <%= yield %>
  </trackList>
</playlist>